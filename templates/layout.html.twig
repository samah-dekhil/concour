/*****************************GET LIST GRADES AVAILABLES *****************************/

    $('#reclamation_grade').empty()
    $.ajax({
      url : '{{path("reclamation_grade_availables")}}',
      type: 'POST',
      success: function(html) {
          /// console.log(html)
          $('#reclamation_grade').empty().trigger('change');
          $('#reclamation_grade').append('<option value=""></option>')
          //   res=JSON.stringify(html)
          let array = Object.entries(html);
          
          if(array.length==0) {
            //console.log('nb grade :'+array.length)
            $('a.reclamation').remove()
            if (window.location.href.indexOf("envoyer-reclamation") > -1) {
             window.location = '{{path("app_home")}}';
             }
          }else{ 
             $('a.reclamation').show()
             }
     

          array.forEach( el => {
            // console.log(el[1].id);
                  $('#reclamation_grade').append('<option value="'+el[1].id+'">'+el[1].libelle+'</option>');

        });
        }
    });

    </script>


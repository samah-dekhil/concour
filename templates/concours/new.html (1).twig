{% extends 'layout_admin.html.twig' %}

{% block body_id 'admin_concours_new' %}

{% block main %}

<div class="card-body">
<h4 class="card-title">Ajouter concours </h4>
      </div>
    {{ form_start(form) }}
        
   

            <div class="col-md-12">
              <div class="card">
              
                  <div class="card-body">
                
                    <div class="form-group row">
                       {{ form_label(form.libelle) }}
                      <div class="input-group">
                       {{ form_widget(form.libelle) }}
                      </div>
                    </div>
                    <div class="form-group row col-md-5">
                        {{ form_label(form.dateDebutInscription) }}
                        <div class="input-group">
                            {{ form_widget(form.dateDebutInscription) }}
                        
                        </div>
                    </div>
                      <div class="form-group row  col-md-5">
                        {{ form_label(form.dateFinInscription) }}
                        <div class="input-group">
                            {{ form_widget(form.dateFinInscription) }}
                       
                        </div>
                    </div>
                  </div>      
                
                  <div class="card-body d-flex justify-content-end">
                        <!--begin::Button-->
                        <a href="{{ path('concours_index') }}" class="btn btn-light me-5">Retour à la liste</a>
                        <!--end::Button-->
                        <!--begin::Button-->
                        <button type="submit" class="btn btn-primary">
                            <span class="indicator-label">Créer</span>
                        </button>
                        <!--end::Button-->
                  </div>
               
              </div>
            
            </div>
   {{ form_end(form) }}
{% endblock %}


{% if app.request.hasPreviousSession %}
    {% for message in app.flashes('success') %}
        <div class="alert alert-success">
            {{ message }}
        </div>
    {% endfor %}
    {% for message in app.flashes('error') %}
        <div class="alert alert-danger" style="    font-size: 22px;">
            {{ message }}
        </div>
    {% endfor %}
{% endif %}
